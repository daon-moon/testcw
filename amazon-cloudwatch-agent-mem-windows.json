{
	"metrics": {
		"append_dimensions": {
			"AutoScalingGroupName": "${aws:AutoScalingGroupName}",
			"InstanceId": "${aws:InstanceId}"
		},
		"aggregation_dimensions": [["InstanceId","device"],["InstanceId"]],
		"metrics_collected": {
		 "Memory": {
            "metrics_collection_interval": 5,
            "measurement": [
            "Available Bytes",
            "Cache Faults/sec",
            "Page Faults/sec",
            "Pages/sec"
            ],
            "append_dimensions": {
          "d3": "win_bo"
        }
      }
		}
	}
}
