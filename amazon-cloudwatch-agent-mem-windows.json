{
	"metrics": {
		"append_dimensions": {
			"AutoScalingGroupName": "${aws:AutoScalingGroupName}",
			"InstanceId": "${aws:InstanceId}"
		},
		"aggregation_dimensions": [["InstanceId","device"],["InstanceId"]],
		"metrics_collected": {
"statsd":{},
"Processor": {
        "measurement": [
	  {"name": "% User Time", "rename": "CPU_USER_TIME", "unit": "Percent"},	
          {"name": "% Processor Time", "rename": "CPU_PROCESSOR_TIME", "unit": "Percent"},
	  {"name": "% Idle Time", "rename": "CPU_IDLE_TIME", "unit": "Percent"},
	  {"name": "% Interrupt Time", "rename": "CPU_INTERRUPT_TIME", "unit": "Percent"}	
        ],
        "metrics_collection_interval": 60,
        "resources": [
          "*"
        ],
        "append_dimensions": {
          "d1": "win_foo",
          "d2": "win_bar"
        }
      },
"LogicalDisk": {
        "measurement": [
	 {"name": "% Free Space", "rename": "DISK_FREE_SPACE", "unit": "Percent"},
	 {"name": "% Idle Time", "rename": "DISK_IDLE_TIME", "unit": "Percent"},
         {"name": "% Disk Read Time", "rename": "DISK_READ_TIME", "unit": "Percent"},
         {"name": "% Disk Write Time", "rename": "DISK_WRITE_TIME", "unit": "Percent"}	
        ],
        "metrics_collection_interval": 60,
        "resources": [
          "*"
        ]
      },
 "Memory": {
         "measurement": [
          {"name": "% Committed Bytes In Use", "rename": "MEM_USAGE", "unit": "Percent"},		 
	  "Available Bytes",
          "Cache Faults/sec",
          "Page Faults/sec",
          "Pages/sec"
        ],
        "metrics_collection_interval": 60,
        "append_dimensions": {
          "d3": "win_bo"
        }
      }
    }
 }
}
